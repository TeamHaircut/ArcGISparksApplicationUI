COLOR
	COLOR_ID INT
	COLOR_NAME VARCHAR(30)
	COLOR_HEX VARCHAR(7)
	
REGION
	REGION_ID INT
	REGION_NAME VARCHAR(30)
	REGION_COLOR_ID INT

DESIGNATION
	DESIGNATION_ID INT
	DESIGNATION_NAME VARCHAR(50)
	DESIGNATION_ABBREVIATION VARCHAR(10)

STATE
	STATE_ID INT
	STATE_NAME VARCHAR(50)
	STATE_ABBREVIATION VARCHAR(2)
	REGION_ID INT

SITE
	SITE_ID INT
	SITE_NAME VARCHAR(50
	DESIGNATION_ID INT
	STATE_ID INT
	LON DOUBLE
	LAT DOUBLE
	WEBSITE VARCHAR(200)
	MAP VARCHAR(200)
	VISITED INT